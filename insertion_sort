#include<iostream>
using namespace std;
int main()
{
	int a[6];
	cout<<"enter elements";
	for(int i=0;i<6;i++)
	{
		cin>>a[i];
	}
	int key;
	int j;
	int i;
	key=a[j];
	i=j-1;
	for(int j=1;j<6;j++)
	{
		key=a[j];
	i=j-1;
		while(a[i]>key && i>=0)
		{
			a[i+1]=a[i];
			i--;
		}
		a[i+1]=key;
		
		}
	for(int i=0;i<10;i++)
	{
		cout<<a[i];
	}
}